<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ysu3_sizebook" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddRecord"
            android:label="@string/title_activity_add_record" >
        </activity>
        <activity
            android:name=".ViewDetail"
            android:label="@string/title_activity_view_detail" >
        </activity>
        <activity
            android:name=".EditRecord"
            android:label="@string/title_activity_edit_record" >
        </activity>
    </application>

</manifest>
